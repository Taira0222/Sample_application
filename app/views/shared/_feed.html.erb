<% if @feed_items.any? %> <%# @feed_itemsはstatic_pagesC から読み取っている %>
    <ol class="microposts">
        <%= render @feed_items %> <%# user.rbのfeedメソッドにより,Micropostクラスのインスタンスとなるので、_micropostパーシャルに飛ぶ %>
    </ol>
    <%= will_paginate @feed_items, params:{ controller: :static_pages, action: :home } %>
<% end %>

